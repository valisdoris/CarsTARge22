@model IEnumerable<CarsTARge22.Models.Cars.CarsIndexViewModel>

<div class="h1">
    Cars
</div>

<form method="post">
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">Nr</th>
                <th scope="col">Brand</th>
                <th scope="col">Type</th>
                <th scope="col">Year</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var car in Model)
            {
                <tr>
                    <td></td>
                    <td>@car.Brand</td>
                    <td>@car.Type</td>
                    <td>@car.Year</td>
                </tr>
            }
        </tbody>
    </table>
</form>